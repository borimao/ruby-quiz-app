<h1>「<%= @user.name%>」さんのホーム</h1>

<table>
  <tr>
    <th>名前</th><td><%= @user.name %></td>
  </tr>
  <tr>
    <th>ID</th><td><%= @user.account_id %></td>
  </tr>
  <tr>
    <th>コメント</th><td><%= @user.memo %></td>
  </tr>
</table>

<hr>
<a href="/questions">戻る</a>
<hr>
<p>「<%= @user.name%>」さんが作ったクイズ</p>
<ul>
<% @quiz.each do |quiz| %>
  <li><a href="/questions/show/<%= quiz.id %>"><%= quiz.title %></a></li>
<% end %>
</ul>