<h1>「<%= @users.name%>」さんのホーム</h1>
<div><%= link_to "ログアウト", destroy_account_session_path(), method: :delete %></div>

<table>
  <tr>
    <th>名前</th><td><%= @users.name %></td>
  </tr>
  <tr>
    <th>ID</th><td><%= @users.account_id %></td>
  </tr>
  <tr>
    <th>コメント</th><td><%= @users.memo %></td>
  </tr>
  <tr>
    <th></th><td><%= link_to 'Edit', edit_user_path(@users) %></td>
  </tr>
</table>

<hr>
<a href="/questions/new/<%= @users.account_id %>">クイズを作る</a><br>
<a href="/questions">戻る</a>
<hr>
<p>「<%= @users.name%>」さんが作ったクイズ</p>
<ul>
<% @quiz.each do |quiz| %>
  <li><a href="/questions/show/<%= quiz.id %>"><%= quiz.title %></a></li>
<% end %>
</ul>
  


